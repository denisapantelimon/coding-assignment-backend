{
  "roots": ["<rootDir>"],
  "modulePathIgnorePatterns": ["<rootDir>/dist"],
  "transform": {
    "^.+\\.tsx?$": "ts-jest"
  },
  "testRegex": "((\\.|/)(test))\\.ts$",
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "node"],
  "verbose": false,
  "silent": true,
  "coverageThreshold": {
    "global": {
      "branches": 90,
      "functions": 90,
      "lines": 90,
      "statements": 90
    }
  }
}